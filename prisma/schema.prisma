

generator client {
  provider = "prisma-client-js"
  output = "./generated/client"
}

datasource db {
  provider = "postgresql" 

}

model ClientRequest {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  subject   String   @db.Text
  message   String 
  createdAt DateTime @default(now())
}